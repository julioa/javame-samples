                    <Property name="items" typeID="1Cjavax.microedition.lcdui.Item" value="A8:4_R1504_R1514_R1524_R1534_R1544_R1554_R1564_R157"/>
                    <Component componentID="152" typeID="Cjavax.microedition.lcdui.StringItem">
